imow:locatieaanduidingIdentifier a dc:Identifier .
imow:workIdentifier a dc:Identifier .
imow:akn imow:workId imow:workId .
imow:wId a dc:identifier .

imow:regeltekstId lvbb:linkToWId imow:workIdentifier ;
    lvbb:linkToRegeling imow:akn ;
    lvbb:linkToWid imow:wId ;
    imow:locatieaanduiding dc:locatieaanduidingIdentifier ;
    imow:artikelOfLid  xsd:string;
    imow:gebiedsaanwijzingId r:gebiedsaanwijzing ;
    imow:activiteitaanduiding r:activiteitaanduiding .

imow:ponsId a dc:Identifier ;
    imow:regelTekstId imow:regelTekstIdentifier ;
    imow:locatieaanduiding dc:locatieaanduidingIdentifier .

rol:omgevingswaardeId imow:regeltekst imow:regelTekstIdentifier .

l:gebiedId imow:regeltekst imow:regelTekstId ;
    l:geometrieId dc:Identifier .

ow:owBestand imow:workId imow:workIdentifier ;
   imow:heeftActiviteit rol:Activiteit ;
   imow:heeftGebied l:gebiedId ;
   imow:heeftOmgevingswaarde rol:omgevingswaardeId ;
   imow:heeftPons imow:ponsId ;
   imow:heeftRegelTekst imow:regelTekstId ;
   lvbb:linkToRegeling imow:akn ; 
   imow:bestand xsd:String ;
   imow:heeftRegelVoorIedereen imow:regelTekstId .

rol:omgevingswaardeId imow:locatieaanduiding dc:locatieaanduidingIdentifier .

rol:Activiteit imow:regeltekst imow:regelTekstId;
    imow:locatieaanduidingId dc:locatieaanduidingIdentifier ;
    imow:bovenliggendeActiviteitId rol:Activiteit .

imow:gebiedsaanwijzingId imow:heeftGebiedsaanwijzing r:gebiedsaanwijzing ;
    imow:regeltekst imow:regelTekstId ;
    imow:locatieaanduidingId dc:locatieaanduidingIdentifier .





    
