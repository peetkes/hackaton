{
  "template": {
    "description": "Extraction Template for gml-bestand",
    "context": "/mow:Aanleveringen/mow:Aanlevering",
    "collections": [
      "/opera/document/manifest-ow"
    ],
    "pathNamespace": [
      {
        "prefix": "mow",
        "namespaceUri": "http://www.geostandaarden.nl/bestanden-ow/manifest-ow"
      }
    ],
    "vars": [
      {
        "name": "lvbb",
        "val": "'http://koop.overheid.nl/ontology/lvbb/'"
      },
      {
        "name": "rdf",
        "val": "'http://www.w3.org/1999/02/22-rdf-syntax-ns#'"
      },
      {
        "name": "rdfs",
        "val": "'http://www.w3.org/2000/01/rdf-schema#'"
      },
      {
        "name": "regelingId",
        "val": "mow:WorkIDRegeling/fn:string()"
      },
      {
        "name": "doelId",
        "val": "mow:DoelID/fn:string()"
      },
      {
        "name": "imow",
        "val": "'http://www.geostandaarden.nl/imow/'"
      },
      {
        "name": "prefix-id",
        "val": "''"
      },
      {
        "name": "prefix-id-lvbb",
        "val": "'http://koop.overheid.nl/ontology/lvbb/id'"
      }
    ],
    "templates": [
      {
        "description": "Extraction Template for ow Bestand",
        "context": "mow:Bestand",
        "vars": [
          {
            "name": "naam",
            "val": "mow:naam/fn:string()"
          },
          {
            "name": "bestandId",
            "val": "sem:uuid()"
          }
        ],
        "triples": [
          {
            "subject": {
              "val": "$bestandId"
            },
            "predicate": {
              "val": "sem:iri($imow || 'bestandsnaam')"
            },
            "object": {
              "val": "$naam"
            }
          },
          {
            "subject": {
              "val": "sem:iri($prefix-id || $regelingId)"
            },
            "predicate": {
              "val": "sem:iri($lvbb || 'linkToOwObject')"
            },
            "object": {
              "val": "sem:iri($bestandId)"
            }
          },
          {
            "subject": {
              "val": "sem:iri($prefix-id || $doelId)"
            },
            "predicate": {
              "val": "sem:iri($lvbb || 'linkToOwObject')"
            },
            "object": {
              "val": "sem:iri($bestandId)"
            }
          }
        ],
        "templates": [
          {
            "description": "Extraction Template for ow Bestand",
            "context": "mow:objecttype",
            "triples": [
              {
                "subject": {
                  "val": "sem:iri($bestandId)"
                },
                "predicate": {
                  "val": "sem:iri($imow || 'heeftObjectType')"
                },
                "object": {
                  "val": "fn:string(.)"
                }
              }
            ]
          }
        ]
      }
    ]
  }
}