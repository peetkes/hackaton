          "val": "sem:iri($imow || 'id/' || $owId)",
          "invalidValues": "ignore"
        },
        "predicate": {
          "val": "sem:iri($imow || 'bestand')"
        },
        "object": {
          "val": "$bestand"
        }
      }
    ],
    "templates": [
      {
        "description": "Extraction Template for Pons ow-bestand",
        "context": "sl:stand/ow-dc:owObject/p:Pons",
        "vars": [{
